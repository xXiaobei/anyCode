    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: anyCode",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "pythonPath": "${config:python.pythonPath}"
        },
        {
            "name": "anyCode Scrapy",
            "type": "python",
            "request": "launch",
            "module": "scrapy",
            "args": [
                "crawl",
                "mirror"
            ],
            "cwd": "${workspaceFolder}/domainmirror/domainmirror"
        }
    ]
}